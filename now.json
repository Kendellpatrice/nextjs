{   "version":2,
    "name":"nextjs",
    "builds":[
        {"src":"package.json","use":"@now/next"}
    ],
    "routes":[
        {"src":"/favicon.ico","dest":"/favicon.ico"},
        {"src":"/_next/(.*)$","dest":"/_next/$1"},
        {"src":"/static/(.*)$","dest":"/static/$1"},
        {"src":"/(.*)$","dest":"/index?page=$1"}
    ]
}